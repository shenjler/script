# 方法1
cat /etc/ssh/sshd_config | grep -v '#' |grep -v "^$"
ssh-keygen -t rsa
ssh root@10.129.22.60 mkdir -p .ssh
cat .ssh/id_rsa.pub | ssh root@10.129.22.60 'cat >> .ssh/authorized_keys'
ssh root@10.129.22.60 'cat .ssh/authorized_keys'

